menu "at_bt"

config AT_BT_CLASSIC_SUPPORT
    bool "AT_BT_CLASSIC_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_BT_CLASSIC_SUPPORT will be defined

config AT_BT_APP_SUPPORT
    bool "AT_BT_APP_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_BT_APP_SUPPORT will be defined

config AT_BLUEU_VERIFY_SUPPORT
    bool "AT_BLUEU_VERIFY_SUPPORT enabled"
    default n
    select BLUEU_BT_ENABLE
    help
        When enabled, CONFIG_AT_BLUEU_VERIFY_SUPPORT will be defined

endmenu
